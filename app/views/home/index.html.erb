<% if @event and !@event.draft? %>
  <% if @event.ended? %>
    <% cache "event_ended_#{@event.id}" do %>
      <%= render partial: '/events/event_ended' %>
    <% end %>
  <% else %>
    <%= render partial: '/events/event' %>
  <% end %>
<% else %>
  <% cache "no_event_#{current_user.id}" do %>
    <%= render partial: '/events/no_event' %>
  <% end %>
<% end %>
